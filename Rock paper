import random
choice = int(input("enter your choice : rock(0), papper(1), scissor(2)"))
ran = random.randint(0,2)
print(ran)
if choice == 0 and ran == 1:
    print("computer wins")
elif choice == 0 and ran == 2:
    print("computer wins")
elif choice == 1 and ran == 2:
    print("computer wins")
elif choice == 2 and ran == 0:
    print ("computer wins")
elif choice == ran:
    print("draw")
else: 
    print("user winsss")
